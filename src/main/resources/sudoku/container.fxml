<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            style="-fx-background-color: #e2e9ee;" xmlns="http://javafx.com/javafx/18" fx:controller="sudoku.ContainerController">
	<children>
		<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0"
		      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<HBox>
					<children>
						<MenuButton maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
						            prefHeight="40.0" style="-fx-background-color: none;" text="New Game">
							<items>
								<MenuItem mnemonicParsing="false" text="Easy"/>
								<MenuItem mnemonicParsing="false" text="Medium"/>
								<MenuItem mnemonicParsing="false" text="Hard"/>
								<MenuItem mnemonicParsing="false" text="Import Sudoku CSV File"/>
							</items>
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</MenuButton>
						<MenuButton maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
						            prefHeight="40.0" style="-fx-background-color: none;" text="Solve">
							<items>
								<MenuItem mnemonicParsing="false" text="Next Move"/>
								<MenuItem mnemonicParsing="false" text="Final Solution"/>
							</items>
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</MenuButton>
						<Pane maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minWidth="-Infinity" HBox.hgrow="ALWAYS"/>
						<Button fx:id="annotationBtn" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
						        minWidth="-Infinity" mnemonicParsing="false" onAction="#toggleAnnotate" prefHeight="40.0" prefWidth="130.0"
						        style="-fx-background-color: none;" text="Annotations: OFF" textAlignment="CENTER">
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</Button>
						<Button fx:id="undoBtn" contentDisplay="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity"
						        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="30.0"
						        style="-fx-background-color: none;">
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</Button>
						<Button fx:id="redoBtn" contentDisplay="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity"
						        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="30.0"
						        style="-fx-background-color: none;">
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</Button>
						<Button fx:id="eraseBtn" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
						        minWidth="-Infinity" mnemonicParsing="false" onAction="#toggleErase" prefHeight="40.0" prefWidth="88.0"
						        style="-fx-background-color: none;" text="Erase: OFF" textAlignment="CENTER">
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</Button>
						<Button fx:id="resetBtn" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
						        minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" style="-fx-background-color: none;" text="Reset">
							<font>
								<Font name="Iosevka 2022 Semibold" size="14.0"/>
							</font>
							<cursor>
								<Cursor fx:constant="HAND"/>
							</cursor>
						</Button>
					</children>
				</HBox>
				<Pane maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="2.0"
				      style="-fx-background-color: #C9CFD4;"/>
				<AnchorPane fx:id="boardAnchor" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
					<VBox.margin>
						<Insets bottom="40.0" left="40.0" right="40.0" top="40.0"/>
					</VBox.margin>
				</AnchorPane>
			</children>
		</VBox>
	</children>
</AnchorPane>
